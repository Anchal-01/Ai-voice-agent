pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                sh 'echo "Building project..."'
            }
        }

        stage('Test') {
            steps {
                sh 'echo "Testing project..."'
            }
        }

        stage('Finish') {
            steps {
                echo "Pipeline done!"
            }
        }
    }
}
